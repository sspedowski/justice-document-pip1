
# Backend Environment Variables
JWT_SECRET="YOUR_VERY_STRONG_JWT_SECRET_HERE_AT_LEAST_32_CHARS_LONG"
ADMIN_USER="admin"
ADMIN_PASS="adminpass" # CHANGE THIS IN PRODUCTION
WOLFRAM_ALPHA_API_KEY="YOUR_WOLFRAM_ALPHA_API_KEY_HERE"
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE"
SESSION_SECRET="YOUR_SESSION_SECRET_FOR_EXPRESS_SESSION"

# Firebase Admin SDK Credentials (JSON content of your service account key file)
# IMPORTANT: DO NOT COMMIT YOUR ACTUAL KEY TO GIT.
# For local development, paste the entire JSON object here.
# For production, set this directly in your hosting environment variables.
# Example (replace with your actual JSON):
FIREBASE_ADMIN_CREDENTIALS_JSON='{
    "type": "service_account",
    "project_id": "your-project-id",
    "private_key_id": "your-private-key-id",
    "private_key": "-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_CONTENT_HERE\n-----END PRIVATE KEY-----\n",
    "client_email": "your-service-account-email@your-project-id.iam.gserviceaccount.com",
    "client_id": "your-client-id",
    "auth_uri": "https://accounts.google.com/o/oauth2/auth",
    "token_uri": "https://oauth2.googleapis.com/token",
    "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
    "client_x509_cert_url": "https://www.googleapis.com/robot/v1/metadata/x509/your-service-account-email.iam.gserviceaccount.com",
    "universe_domain": "googleapis.com"
}'

# Firebase Storage Bucket for Admin SDK
FIREBASE_STORAGE_BUCKET="your-project-id.appspot.com"

# Frontend Firebase Configuration (Client-side Firebase config, from your Firebase console)
# These are safe to be exposed in frontend code.
VITE_FIREBASE_API_KEY="YOUR_WEB_API_KEY"
VITE_FIREBASE_AUTH_DOMAIN="YOUR_AUTH_DOMAIN"
VITE_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
VITE_FIREBASE_STORAGE_BUCKET="YOUR_STORAGE_BUCKET"
VITE_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
VITE_FIREBASE_APP_ID="YOUR_APP_ID"
VITE_FIREBASE_MEASUREMENT_ID="YOUR_MEASUREMENT_ID"
