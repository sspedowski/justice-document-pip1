# Justice Dashboard Environment Configuration
# Copy this file to .env and fill in your actual values

# Security Secrets (REQUIRED - must be 32+ characters)
JWT_SECRET=your-super-secure-jwt-secret-key-here-32-chars-minimum
SESSION_SECRET=your-super-secure-session-secret-key-here-32-chars-minimum

# OpenAI API (Optional - for AI summarization)
OPENAI_API_KEY=your-openai-api-key-here

# Server Configuration (Optional)
PORT=3000
NODE_ENV=development

# Frontend URL (Optional)
FRONTEND_URL=http://localhost:5174
